    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Group A Streptococcus Lab Test With Result"
      }, {
        "url": "statement",
        "valueString": "define \"Group A Streptococcus Lab Test With Result\":\n  ( Status.\"Final Lab Observation\" ( [Observation: \"Group A Streptococcus Test\"] ) ) GroupAStreptococcusTest\n    where GroupAStreptococcusTest.value is not null"
      }, {
        "url": "displaySequence",
        "valueInteger": 0
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Qualifying Encounter"
      }, {
        "url": "statement",
        "valueString": "define \"Qualifying Encounter\":\n  ( Status.\"Finished Encounter\" ( [Encounter: \"Emergency Department Visit\"] \n// //   [Encounter: \"Observation care discharge day management (This code is to be utilized to report all services provided to a patient on discharge from outpatient hospital observation status if the discharge is on other than the initial date of observation status. To report services to a patient designated as observation status or inpatient status and discharged on the same date, use the codes for Observation or Inpatient Care Services [including Admission and Discharge Services, 99234-99236 as appropriate.])\"]\n//     //union\n//     [Encounter: \"Emergency Department Visit\"] \n    \n    \n    \n    union [\"Encounter\": \"Home Healthcare Services\"]\n    union [\"Encounter\": \"Initial Hospital Observation Care\"]\n    union [\"Encounter\": \"Medical Disability Exam\"]\n    union [\"Encounter\": \"Observation\"]\n    union [\"Encounter\": \"Office Visit\"]\n    union [\"Encounter\": \"Telephone Visits\"]\n    union [\"Encounter\": \"Online Assessments\"]\n    union [\"Encounter\": \"Outpatient Consultation\"]\n    union [\"Encounter\": \"Preventive Care Services Established Office Visit, 18 and Up\"]\n    union [\"Encounter\": \"Preventive Care Services Group Counseling\"]\n    //union [\"Encounter\": \"Unlisted preventive medicine service\"]\n    \n    union [\"Encounter\": \"Preventive Care Services, Initial Office Visit, 0 to 17\"]\n    union [\"Encounter\": \"Preventive Care Services Individual Counseling\"]\n    union [\"Encounter\": \"Preventive Care Services Initial Office Visit, 18 and Up\"]\n    union [\"Encounter\": \"Preventive Care, Established Office Visit, 0 to 17\"] ) ) ValidEncounter\n    where QICoreCommon.\"ToInterval\" ( ValidEncounter.period ) during \"Measurement Period\""
      }, {
        "url": "displaySequence",
        "valueInteger": 1
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Encounter With Antibiotic Ordered Within Three Days"
      }, {
        "url": "statement",
        "valueString": "define \"Encounter With Antibiotic Ordered Within Three Days\":\n  \"Qualifying Encounter\" EDOrAmbulatoryVisit\n    with ( Status.\"Active Medication\" ( [MedicationRequest: \"Antibiotic Medications for Pharyngitis\"] ) ) AntibioticOrdered\n      such that ( QICoreCommon.\"ToInterval\" ( EDOrAmbulatoryVisit.period ) starts 3 days or less on or before AntibioticOrdered.authoredOn )"
      }, {
        "url": "displaySequence",
        "valueInteger": 2
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Pharyngitis or Tonsillitis"
      }, {
        "url": "statement",
        "valueString": "define \"Pharyngitis or Tonsillitis\":\n  ( Status.\"Active Condition\" ( [Condition: \"Acute Pharyngitis\"]\n      union [Condition: \"Acute Tonsillitis\"] )\n  )"
      }, {
        "url": "displaySequence",
        "valueInteger": 3
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Encounter With Pharyngitis or Tonsillitis With Antibiotic"
      }, {
        "url": "statement",
        "valueString": "define \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\":\n  from\n    \"Encounter With Antibiotic Ordered Within Three Days\" VisitWithAntibiotic,\n    \"Pharyngitis or Tonsillitis\" AcutePharyngitisTonsillitis\n    where QICoreCommon.\"ToPrevalenceInterval\" ( AcutePharyngitisTonsillitis ) starts during QICoreCommon.\"ToInterval\" ( VisitWithAntibiotic.period )\n    return VisitWithAntibiotic"
      }, {
        "url": "displaySequence",
        "valueInteger": 4
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Numerator"
      }, {
        "url": "statement",
        "valueString": "define \"Numerator\":\n  from\n    \"Group A Streptococcus Lab Test With Result\" GroupAStreptococcusTest,\n    \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\" EncounterWithPharyngitis\n    where ( QICoreCommon.\"ToInterval\" ( GroupAStreptococcusTest.effective ) starts during day of Interval[( end of EncounterWithPharyngitis.period ) - 3 days, ( end of EncounterWithPharyngitis.period ) + 3 days])\n    return EncounterWithPharyngitis"
      }, {
        "url": "displaySequence",
        "valueInteger": 5
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Stratification 3"
      }, {
        "url": "statement",
        "valueString": "define \"Stratification 3\":\n  \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\" EncounterWithPharyngitis\n    where AgeInYearsAt(date from start of \"Measurement Period\") >= 65\n    return EncounterWithPharyngitis"
      }, {
        "url": "displaySequence",
        "valueInteger": 6
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Stratification 2"
      }, {
        "url": "statement",
        "valueString": "define \"Stratification 2\":\n  \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\" EncounterWithPharyngitis\n    where AgeInYearsAt(date from start of \"Measurement Period\") in Interval[18, 64]\n    return EncounterWithPharyngitis"
      }, {
        "url": "displaySequence",
        "valueInteger": 7
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Stratification 1"
      }, {
        "url": "statement",
        "valueString": "define \"Stratification 1\":\n  \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\" EncounterWithPharyngitis\n    where AgeInYearsAt(date from start of \"Measurement Period\") in Interval[3, 17]\n    return EncounterWithPharyngitis"
      }, {
        "url": "displaySequence",
        "valueInteger": 8
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Initial Population"
      }, {
        "url": "statement",
        "valueString": "define \"Initial Population\":\n  \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\" EncounterWithPharyngitis\n    where AgeInYearsAt(date from start of \"Measurement Period\") >= 3\n    return EncounterWithPharyngitis"
      }, {
        "url": "displaySequence",
        "valueInteger": 9
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Denominator"
      }, {
        "url": "statement",
        "valueString": "define \"Denominator\":\n  \"Initial Population\""
      }, {
        "url": "displaySequence",
        "valueInteger": 10
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "Hospice"
      }, {
        "url": "name",
        "valueString": "Has Hospice Services"
      }, {
        "url": "statement",
        "valueString": "define \"Has Hospice Services\":\n  exists ( ( Status.\"Finished Encounter\" ( [Encounter: \"Encounter Inpatient\"] ) ) InpatientEncounter\n      where ( InpatientEncounter.hospitalization.dischargeDisposition ~ \"Discharge to home for hospice care (procedure)\"\n          or InpatientEncounter.hospitalization.dischargeDisposition ~ \"Discharge to healthcare facility for hospice care (procedure)\"\n      )\n        and QICoreCommon.\"ToInterval\" ( InpatientEncounter.period ) ends during day of \"Measurement Period\"\n  )\n    or exists ( ( Status.\"Finished Encounter\" ( [Encounter: \"Hospice Encounter\"] ) ) HospiceEncounter\n        where QICoreCommon.\"ToInterval\" ( HospiceEncounter.period ) overlaps \"Measurement Period\"\n    )\n    or exists ( ( Status.\"Final Survey Observation\" ( [Observation: \"Hospice care [Minimum Data Set]\"] ) ) HospiceAssessment\n        where HospiceAssessment.value ~ \"Yes (qualifier value)\"\n          and QICoreCommon.\"ToInterval\" ( HospiceAssessment.effective ) overlaps \"Measurement Period\"\n    )\n    or exists ( ( Status.\"Completed or Ongoing Service Request\" ( [ServiceRequest: code in \"Hospice Care Ambulatory\"] ) ) HospiceOrder\n        where QICoreCommon.\"ToInterval\" ( HospiceOrder.authoredOn ) during day of \"Measurement Period\"\n    )\n    or exists ( ( Status.\"Completed Procedure\" ( [Procedure: \"Hospice Care Ambulatory\"] ) ) HospicePerformed\n        where QICoreCommon.\"ToInterval\" ( HospicePerformed.performed ) overlaps \"Measurement Period\"\n    )"
      }, {
        "url": "displaySequence",
        "valueInteger": 11
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "In Hospice"
      }, {
        "url": "statement",
        "valueString": "define \"In Hospice\":\n  \"Initial Population\" EligibleEncounters\n    where Hospice.\"Has Hospice Services\""
      }, {
        "url": "displaySequence",
        "valueInteger": 12
      } ]
    }, {
      "url": "http://hl7.org/fhir/us/cqfmeasures/StructureDefinition/cqfm-logicDefinition",
      "extension": [ {
        "url": "libraryName",
        "valueString": "AppropriateTestingforPharyngitisFHIR"
      }, {
        "url": "name",
        "valueString": "Denominator Exclusions"
      }, {
        "url": "statement",
        "valueString": "define \"Denominator Exclusions\":\n  \"In Hospice\"\n    union Antibiotic.\"Has Antibiotic Medication History\" ( \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\", ( [MedicationRequest: \"Antibiotic Medications for Pharyngitis\"] ) )\n    union Antibiotic.\"Has Competing Diagnosis History\" ( \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\", ( [Condition: \"Competing Conditions for Respiratory Conditions\"] ) )\n    union Antibiotic.\"Has Comorbid Condition History\" ( \"Encounter With Pharyngitis or Tonsillitis With Antibiotic\", ( [Condition: \"Comorbid Conditions for Respiratory Conditions\"] ) )"
      }, {
        "url": "displaySequence",
        "valueInteger": 13
      } ]
    } ],
